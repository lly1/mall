<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mall.dao.user.RoleMapper" >
    <select id="getUserRoles" resultType="Role" parameterType="java.lang.String">
        select * from sys_role a,sys_user_role b where a.role_id = b.role_id and b.user_id = #{userId} and a.delFlag = 0
    </select>

</mapper>


