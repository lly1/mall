<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mall.dao.menu.MenuMapper" >
    <select id="getMenuByRole" resultType="Menu" parameterType="java.lang.Integer">
        select * from sys_menu a join sys_role_menu b on a.code = b.menuId where b.roleId = #{roleId}
    </select>

</mapper>


